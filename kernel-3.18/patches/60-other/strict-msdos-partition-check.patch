diff -Naur orig/block/partitions/msdos.c patched/block/partitions/msdos.c
--- orig/block/partitions/msdos.c	2015-05-20 17:04:50.000000000 +0200
+++ patched/block/partitions/msdos.c	2015-05-22 18:33:52.168814778 +0200
@@ -57,7 +57,8 @@
 static inline int
 msdos_magic_present(unsigned char *p)
 {
-	return (p[0] == MSDOS_LABEL_MAGIC1 && p[1] == MSDOS_LABEL_MAGIC2);
+	return ((p[510] == MSDOS_LABEL_MAGIC1 && p[511] == MSDOS_LABEL_MAGIC2)
